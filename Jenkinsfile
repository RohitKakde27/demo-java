pipeline {
    agent any
    stages {
        stage('Remove Code') {
            steps {
                sh 'sudo rm -rf /var/lib/jenkins/workspace/SonarQube/demo-java'
            }
    stages {
        stage('PULL-PHASE') {
            steps {
                sh 'git clone https://github.com/RohitKakde27/demo-java.git'
            }
        }
    }
}
